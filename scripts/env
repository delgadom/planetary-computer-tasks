#!/bin/bash


PACKAGE_DIRS=(
    "pctasks/core"
    "pctasks/cli"
    "pctasks/task"
    "pctasks/submit"
    "pctasks/ingest"
    "pctasks/ingest_task"
    "pctasks/dataset"
