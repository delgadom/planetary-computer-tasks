    "pctasks/notify"
    "pctasks/router"
    "pctasks/operations"
    "pctasks/records"
    "pctasks/server"
    "pctasks/dev"
)

# All directories of pctasks_funcs that have an __init__.py file
FUNCTION_DIRS=($(find pctasks_funcs -type d -exec test -e "./{}/__init__.py" \; -print))

INTEGRATION_TESTS=(
    "tests"
)
